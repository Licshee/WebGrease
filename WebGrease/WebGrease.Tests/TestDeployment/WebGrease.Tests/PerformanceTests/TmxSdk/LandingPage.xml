      <OutputUnit>rem</OutputUnit>
      <OutputUnitFactor>0.1</OutputUnitFactor>
      <ErrorOnInvalidSprite>false</ErrorOnInvalidSprite>
      <ImagesToIgnore>/images/adchoices/*</ImagesToIgnore>
    </Spriting>
    <Spriting config="Release">
      <OutputUnit>rem</OutputUnit>
      <OutputUnitFactor>0.1</OutputUnitFactor>
      <ErrorOnInvalidSprite>false</ErrorOnInvalidSprite>
      <ImagesToIgnore>/images/adchoices/*</ImagesToIgnore>
