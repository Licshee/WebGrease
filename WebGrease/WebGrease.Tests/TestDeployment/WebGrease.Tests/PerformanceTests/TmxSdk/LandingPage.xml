<?xml version="1.0" encoding="utf-8"?>
<WebGrease>
  <Settings>
    <ImageDirectories>Images</ImageDirectories>
    <ImageExtensions>*.png;*.jpg;*.jpeg;*.gif;*.ico;*.svg</ImageExtensions>
    <TokensDirectory>Shared</TokensDirectory>
    <OverrideTokensDirectory>Resources</OverrideTokensDirectory>
    <!-- these settings apply to each file set defined in this config -->
    <ResourcePivot key="locales" applyMode="CssApplyAfterParse">generic-generic;ja-jp;en-us;en-ca;en-au;en-in;es-ar;es-cl;es-co;de-de;nl-nl</ResourcePivot>
    <ResourcePivot key="themes" applyMode="CssApplyAfterParse">msnblue</ResourcePivot>
    
    <Dpi>1</Dpi>
    <Dpi output="tmx.pc">1;1.4;1.8;2</Dpi>
    <Dpi output="tmx.mobile.ms">1.5;2.25;2.4</Dpi>
    <Dpi output="tmx.mobile.webkit">1;2</Dpi>
    
    <OutputPathFormat>{themes}/{locales}/{dpi}/{output}</OutputPathFormat>
    
    <CssMinification config="Debug">
      <Minify>False</Minify>
      <ValidateLowerCase>True</ValidateLowerCase>
    </CssMinification>
    <CssMinification config="Release">
      <Minify>True</Minify>
      <ValidateLowerCase>True</ValidateLowerCase>
    </CssMinification>
  </Settings>
  <CssFileSet output="landingpage.tmx.pc.ms.css">
