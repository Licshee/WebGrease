<?xml version="1.0" encoding="utf-8"?>
<WebGrease>
  <Settings>
    <TokensDirectory>Shared</TokensDirectory>
    <OverrideTokensDirectory>Resources</OverrideTokensDirectory>
    <!-- these settings apply to each file set defined in this config -->
    <Locales>generic-generic;ja-jp;en-us;en-ca;en-au;en-in;es-ar;es-cl;es-co;de-de;nl-nl;</Locales>
    <JsMinification config="Release">
      <Minify>True</Minify>
      <MinifyArguments>-h -debug:F -fnames:onlyref -rename:all -unused:remove</MinifyArguments>
      <GlobalsToIgnore>jQuery;Msn;define;require</GlobalsToIgnore>
    </JsMinification>
    <JsMinification config="Debug">
      <Minify>False</Minify>
      <MinifyArguments>-debug -p</MinifyArguments>
      <GlobalsToIgnore>jQuery;Msn;define;require</GlobalsToIgnore>
    </JsMinification>
  </Settings>
